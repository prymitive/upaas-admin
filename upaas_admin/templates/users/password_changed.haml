- extends 'site.html'


- load i18n
- load django_bootstrap_breadcrumbs


- block breadcrumbs
  = block.super
  - breadcrumb user.full_name_or_login "users_profile"
  - breadcrumb "Password changed" "password_changed"


- block content

  %h1.text-success
    .text-center
      %i.fa.fa-check-circle-o
      - trans "Password changed successfully"
